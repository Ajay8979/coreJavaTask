package com.ojas.question15;

public class Customer extends ShipmentEntity {
	private int id;
	private String name;

	@Override
	public String toString() {
		return "Customer [id=" + id + ", name=" + name + "]";
	}

	public Customer(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public Customer() {
		super();
	}

	public Customer(String shipmentEntityName, String identificationNumber, int id, String name) {
		this.id = id;
		this.name = name;
		this.identificationNumber = identificationNumber;
		this.shipmentEntityName = shipmentEntityName;
	}
	void display() {
		System.out.println(String.format("%-15s %-25s %-15s %-15s %-15s\n","Name","Identification Number","Company Name","IATA","FMC"));
	}
}
