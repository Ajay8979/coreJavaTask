package com.ojas.question20;

public class ShipmentStatus {
	private	String code;
	private	String name;
	@Override
	public String toString() {
		return "ShipmentStatus [code=" + code + ", name=" + name + "]";
	}
	public ShipmentStatus(String code, String name) {
		super();
		this.code = code;
		this.name = name;
	}
	

}
